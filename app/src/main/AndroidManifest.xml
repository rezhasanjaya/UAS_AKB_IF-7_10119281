        android:supportsRtl="true"
        android:theme="@style/Theme.AKBUAS_10119281"
        tools:targetApi="31">
        <activity
            android:name=".View.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Auth.Login"
            android:exported="true">
        </activity>
        <activity
            android:name=".Presenter.InputActivity"
            android:exported="true">
        </activity>
        <activity
            android:name=".Auth.Register"
            android:exported="true">
        </activity>
        <activity
            android:name=".Presenter.MainActivity"
            android:exported="true">
        </activity>
    </application>

</manifest>